<div class="container">
    <h3>Đăng khóa học mới</h3>
    <form method="POST" action="/courses/store">
    <div class="form-group">
        <label for="name">Tên Khóa học</label>
        <input type="text" class="form-control" id="name" name="name">
    </div>
    <div class="form-group">
        <label for="name">Mô Tả</label>
        <input type="text" class="form-control" id="description" name="description">
    </div>
    <div class="form-group">
        <label for="name">VideoId</label>
        <input type="text" class="form-control" id="videoId" name="videoId">
    </div>
    <div class="form-group">
        <label for="name">Trình Độ</label>
        <input type="text" class="form-control" id="levels" name="levels">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>